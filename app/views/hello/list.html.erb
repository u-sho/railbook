<table>
    <tr>
        <th>ISBN</th>
        <th>Title</th>
        <th>Price</th>
        <th>Publish</th>
        <th>Published</th>
        <th>Download</th>
    </tr>
<% @books.each do |book| %>
    <tr>
        <td><%= book.isbn %></td>
        <td>『<%= book.title %>』</td>
        <td><%= book.price %>円</td>
        <td><%= book.publish %></td>
        <td><%= book.published %></td>
        <td><%= book.dl %></td>
    </tr>
<% end %>
</table>
        
